{
  "name": "interval-cache",
  "version": "0.0.2",
  "description":
    "a cache with synchronous fetches but asynchronous background updates",
  "author": "Dan Neumann <danrodneu@gmail.com> (https://github.com/danneu)",
  "homepage": "https://github.com/danneu/interval-cache",
  "repository": {
    "type": "git",
    "url": "https://github.com/danneu/interval-cache"
  },
  "scripts": {
    "test":
      "echo 'npm test -- --watch for watching' && node ./node_modules/.bin/ava",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master",
    "pretty-fix": "./node_modules/.bin/prettier --write \"{src,test}/**/*.js\""
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=8.0"
  },
  "keywords": ["cache"],
  "dependencies": {
    "better-assert": "^1.0.2",
    "debug": "^3.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "lolex": "^1.6.0",
    "prettier": "^1.7.4",
    "sinon": "^2.2.0"
  }
}
